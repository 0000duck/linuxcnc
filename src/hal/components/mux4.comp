component mux4 "Select from one of four input values";
pin in bit sel0;
pin in bit sel1;
pin out float out;
pin in float in0;
pin in float in1;
pin in float in2;
pin in float in3;
function _;
;;
MODULE_LICENSE("GPL");
FUNCTION(_) {
    if(sel0) {
        if(sel1) out = in3;
        else out = in2;
    } else {
        if(sel1) out = in1;
        else out = in0;
    }
}
