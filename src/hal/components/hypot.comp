component hypot "Three-input hypotenuse (Euclidean distance) calculator";
pin in float in0;
pin in float in1;
pin in float in2;
pin out float out "out = sqrt(in0² + in1² + in2²)";
function _;
license "GPL";
option asciidoc;
;;
#include <rtapi_math.h>
FUNCTION(_) {
    double a = in0, b = in1, c = in2;
    out = sqrt(a*a + b*b + c*c);
}
