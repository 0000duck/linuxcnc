TARGETS += ../bin/rs274
SAISRCS := $(addprefix emc/sai/, saicanon.cc driver.cc dummyemcstat.cc) \
	emc/rs274ngc/tool_parse.cc emc/task/taskmodule.cc
USERSRCS += $(SAISRCS)

$(call TOOBJSDEPS,$(SAISRCS)) : 
../bin/rs274: $(call TOOBJS, $(SAISRCS)) ../lib/librs274.so.0 ../lib/libemc.a ../lib/libnml.so.0
	$(ECHO) Linking $(notdir $@)
	$(CC) $(LDFLAGS)  -o $@ $^ $(ULFLAGS) /usr/lib/libpython2.6.a
