#: m-code.txt:695
#, no-wrap
msgid ""
"#!/bin/bash\n"
"# file to turn off parport pin 14 to open the collet closer\n"
"halcmd setp parport.0.pin-14-out False\n"
"exit 0\n"
"----\n"
msgstr ""
"#!/bin/bash\n"
"# ce fichier met la broche 14 du port à 0 pour fermer le mandrin automatique\n"
"halcmd setp parport.0.pin-14-out False\n"
"exit 0----\n"

